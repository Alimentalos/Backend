{
    "name": "alimentalos/backend",
    "type": "project",
    "description": "Alimentalos -> 'pet care' backend project.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "./contracts"
        },
        {
            "type": "path",
            "url": "./relationships"
        },
        {
            "type": "path",
            "url": "./tools"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^2.7",
        "alimentalos/contracts": "*",
        "alimentalos/relationships": "*",
        "alimentalos/tools": "*",
        "beyondcode/laravel-websockets": "^2.0",
        "cybercog/laravel-love": "^8.7",
        "darkaonline/l5-swagger": "^8.0",
        "igaster/laravel_cities": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/markdown": "^13.0",
        "grimzy/laravel-mysql-spatial": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel-frontend-presets/tall": "^2.0",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.0",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "pusher/pusher-php-server": "~4.0",
        "sentry/sentry-laravel": "^2.0",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "fzaninotto/faker": "^1.9.1",
        "laravel/ui": "^3.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php",
            "app/relationships.php",
            "app/tools.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
