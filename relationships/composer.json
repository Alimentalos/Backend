{
    "name": "alimentalos/relationships",
    "description": "Laravel package for common relationships.",
    "keywords": [
        "laravel",
        "common",
        "relationships",
        "package"
    ],
    "authors": [
        {
            "name": "Ian Torres",
            "email": "iantorres@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "alimentalos/contracts": "*",
        "grimzy/laravel-mysql-spatial": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Alimentalos\\Relationships\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alimentalos\\Relationships\\RelationshipsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
