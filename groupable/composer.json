{
    "name": "demency/laravel-groupable",
    "description": "Laravel package for groupable relationships.",
    "keywords": [
        "laravel",
        "groups",
        "package"
    ],
    "authors": [
        {
            "name": "Ian Torres",
            "email": "iantorres@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Demency\\Groupable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Demency\\Groupable\\GroupableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
